{
  "name": "AQS-API",
  "version": "0.0.1",
  "description": "API for the air quality sensor network using DTN",
  "main": "index.js",
  "repository": "https://github.com/dzvid/aqs-api",
  "author": "David",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "node dist/shared/infra/http/server.js",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --watch ./src/swagger.yaml --ignore-watch node_modules --respawn ./src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "test:integration": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:unit": "NODE_ENV=test jest",
    "seed:admin": "ts-node-dev ./src/shared/infra/typeorm/seeds/admin.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.4",
    "dayjs": "^1.10.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.3",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.0",
    "prettier": "^2.3.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
